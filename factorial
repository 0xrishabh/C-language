#include <stdio.h>
#include <stdlib.h>

int main(){
    int num;
    int fact=1;
    printf("ENTER ANY NUMBER");
    scanf("%d",&num);
    if (num == 0){printf(0);}
    else if(num == 1){printf(1);}
    else{
        for(int i=2;i<=num;i++){
            fact = i*fact;
        }
        printf("%d",fact);
    }
}
